	public static String[] getQid()
	{
		String[] Qid=new String [100];
		int i=0;
		List <WebElement> li=Common.LaunchApp.driver.findElements(By.xpath("//*[@value='0']"));
		for(WebElement element : li){
			try
			{
				Qid[i]=element.getAttribute("name");
				i++;
			}
			catch(Exception e)
			{
				e.printStackTrace();
			}
		}
		return Qid;
		
	}
	
}
