 class CoonectDB{
 
	public static void main(String args[]){  
		
	try{  
		//Class.forName("com.microsoft.sqlserver.jdbc.SQLServerDriver");	
		//Connection con = DriverManager.getConnection("jdbc:sqlserver://INPOD;user=sa;password=p@ssw0rd000;database=SARTierII_ProdDB");
		System.out.println("test");
	Class.forName("com.mysql.jdbc.Driver");  
	Connection con=DriverManager.getConnection("jdbc:mysql://localhost:3306//aspnetdb","sa","p@ssw0rd000");  
	//here sonoo is database name, root is username and password  INPOD
	Statement stmt=con.createStatement();  
